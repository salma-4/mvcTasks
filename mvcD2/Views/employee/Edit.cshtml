
@model Employee

@{
    Layout = null;
}



<h1>EDIT EMPLOYEE </h1>

<Form action="/Employee/EditInDB">
    <label for="Id">Name</label>
    <input type="text" id="Id" name="Id" value="@Model.Id" readonly />
    <br />

    <label for="Name">Name</label>
    <input type="text" id="Name" name="Name" value="@Model.Name" />
    <br />

    <label for="Age">Age</label>
    <input type="number" id="Age" name="Age" value="@Model.Age" />
    <br />


    <label for="Address">Address</label>
    <input type="text" id="Address" name="Address" value="@Model.Address" />
    <br />

    <label for="email">Email</label>
    <input type="text" id="email" name="email" value="@Model.email" />
    <br />


    <label for="password">Password</label>
    <input type="text" id="password" name="password" value="@Model.password" />
    <br />
    <label for="office_id">office_id</label>
    <input type="text" id="office_id" name="office_id" value="@Model.office_id" />
    <br />
    <input type="submit" value="Edit" />
</Form>